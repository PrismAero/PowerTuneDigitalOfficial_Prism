{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerTune (Debug)",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/macos-dev/PowerTuneQMLGui.app/Contents/MacOS/PowerTuneQMLGui",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build (Debug)",
            "env": {
                "QT_QPA_PLATFORM": "cocoa",
                "QML_IMPORT_TRACE": "0",
                "QT_QUICK_CONTROLS_STYLE": "Material"
            }
        },
        {
            "name": "PowerTune (Debug - QML Trace)",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/macos-dev/PowerTuneQMLGui.app/Contents/MacOS/PowerTuneQMLGui",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build (Debug)",
            "env": {
                "QT_QPA_PLATFORM": "cocoa",
                "QML_IMPORT_TRACE": "1",
                "QT_QUICK_CONTROLS_STYLE": "Material"
            }
        },
        {
            "name": "PowerTune (Attach)",
            "type": "lldb",
            "request": "attach",
            "program": "${workspaceFolder}/build/macos-dev/PowerTuneQMLGui.app/Contents/MacOS/PowerTuneQMLGui"
        }
    ]
